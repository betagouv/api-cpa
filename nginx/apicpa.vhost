server {
        listen   80;

        root /var/www/app;
        index index.html;

        server_name ~^cpa(-dev|-test|-local|)\.api\.gouv\.fr$ default;


        location / {

          proxy_set_header X-Real-IP  $remote_addr;
          proxy_set_header X-Forwarded-For $remote_addr;
          proxy_set_header Host $host;
          proxy_pass http://data:3000;

        }

}
